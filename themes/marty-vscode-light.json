	"tokenColors": [
		{
            "scope": [
                "emphasis"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "header"
            ],
            "settings": {
                "foreground": "#2B62D1"
            }
        },
        {
            "scope": [
                "source"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#708787"
            }
        },
        {
            "scope": [
                "markup.inserted"
